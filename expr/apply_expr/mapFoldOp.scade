function mapFoldOp(in_ival1 : int32; in_ival2 : int32)
  returns (o_ival1 : int32; o_ival2 : int32; arr : int32^10)
let  
  o_ival1, o_ival2, arr = (mapfold 2 subOp <<10>>)(in_ival1, in_ival2);
tel

function subOp(ival1 : int32; ival2 : int32)
  returns (o_ival1 : int32; o_ival2 : int32; o_ival3 : int32)
let
  o_ival1 = ival1;
  o_ival2 = ival2;
  o_ival3 = ival1 + ival2;
tel